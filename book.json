{
  "root": ".",
  "title": "GitBook详细教程",
  "author": "JiangMing-JIM",
  "description": "Gitbook的学习和使用详解",
  "language": "zh-hans",
  "links": {
    "sidebar": {
      "个人主页": "",
      "GitHub": "https://github.com/jiangminggithub",
      "Gitee": "https://gitee.com/jiangming_gitee"
    }
  },
  "variables": {
    "blog": "https://blog.csdn.net/ming_97y"
  },
  "pdf": {
    "pageNumbers": true,
    "fontFamily": "Arial",
    "fontSize": 16,
    "paperSize": "a4",
    "margin": {
      "right": 0,
      "left": 0,
      "top": 0,
      "bottom": 0
    }
  },
  "plugins": [
    "-search",
    "-lunr",
    "-sharing",
    "-highlight",
    "insert-logo",
    "favicon",
    "search-pro",
    "splitter",
    "github",
    "sharing-plus",
    "-copy-code-button",
    "code",
    "advanced-emoji",
    "emphasize",
    "image-captions",
    "anchor-navigation-expand",
    "alerts",
    "flexible-alerts",
    "auto-scroll-table",
    "lightbox",
    "-popup",
    "tbfed-pagefooter",
    "-page-footer",
    "hide-element",
    "-back-to-top-button",
    "prism",
    "chapter-fold",
    "-expandable-chapters-small",
    "-expandable-chapters",
    "donate"
  ],
  "pluginsConfig": {
    "insert-logo": {
      "url": "images/icon/logo-jim.png",
      "style": "background: none; max-height: 100px; min-height: 30px"
    },
    "favicon": {
      "shortcut": "./images/icon/favicon.png",
      "bookmark": "./images/icon/favicon.png",
      "appleTouch": "./images/icon/favicon.png",
      "appleTouchMore": {
        "120x120": "./images/icon/favicon.png",
        "180x180": "./images/icon/favicon.png"
      }
    },
    "github": {
      "url": "https://github.com/jiangminggithub"
    },
    "sharing": {
      "douban": false,
      "facebook": false,
      "google": true,
      "pocket": true,
      "qq": true,
      "qzone": true,
      "twitter": true,
      "weibo": true,
      "all": []
    },
    "code": {
      "copyButtons": true
    },
    "image-captions": {
      "caption": "Image _PAGE_LEVEL_._PAGE_IMAGE_NUMBER_ - _CAPTION_",
      "align": "left"
    },
    "flexible-alerts": {
      "style": "callout",
      "comment": {
        "label": "Comment",
        "icon": "fa fa-comments",
        "className": "info"
      }
    },
    "lightbox": {
      "includeJQuery": false,
      "sameUuid": true,
      "options": {
        "resizeDuration": 200,
        "wrapAround": true
      }
    },
    "tbfed-pagefooter": {
      "copyright": "Copyright ©JiangMing",
      "modify_label": "更新时间：",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "hide-element": {
      "elements": [
        ".gitbook-link"
      ]
    },
    "prism": {
      "css": [
        "prismjs/themes/prism-tomorrow.css"
      ]
    },
    "donate": {
      "wechat": "./images/wechat-qr.png",
      "alipay": "./images/alipay-qr.png",
      "title": "",
      "button": "赞赏",
      "wechatText": "微信打赏",
      "alipayText": "支付宝打赏"
    }
  }
}